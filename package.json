{
  "name": "acme-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "base/admin-frontend",
    "base/backend",
    "base/templates/*"
  ],
  "scripts": {
    "dev": "npm run dev -w base/admin-frontend",
    "build": "npm run build -w base/admin-frontend",
    "start": "npm run start -w base/admin-frontend",
    "dev:frontend": "npm run dev -w base/templates/frontend-template",
    "dev:backend": "npm run start -w base/backend"
  },
  "dependencies": {
    "@acme/templates": "^0.1.0"
  },
  "packageManager": "npm@10.9.0"
}
