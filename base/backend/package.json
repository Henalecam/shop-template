{
  "name": "multi-tenant-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "prestart": "node scripts/db-init.js",
    "start:no-db": "node src/index.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate deploy",
    "db:init": "node scripts/db-init.js",
    "test:prisma": "node test-prisma.js",
    "seed": "node --import=dotenv/config prisma/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "prisma": "^5.16.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}